{
  "name": "nean-stack-starter",
  "version": "0.0.763",
  "description": "A sample Angular, Node.js and Neo4j demo app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j",
    "heroku"
  ],
  "engines": {
    "node": "10.9.x",
    "npm": "6.2.x"
  },
  "scripts": {
    "dev": "concurrently \"npm run start:client:watch\" \"npm run build:server:watch\" \"npm run start:server:watch\"",
    "build": "ng build --prod && webpack --mode=production",
    "build:server": "webpack --mode=development",
    "build:server:watch": "webpack --mode=development --watch",
    "start": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/app.bundle.js",
    "start:client:watch": "ng serve --aot",
    "start:server:watch": "nodemon --inspect --max_semi_space_size=2 --max_old_space_size=256 dist/server/app.bundle.js",
    "heroku-postbuild": "npm run build",
    "ng": "ng",
    "test": "echo \"Error: no test... minimal project\" && exit 1",
    "lint": "ng lint",
    "e2e": "echo \"Error: no e2e... minimal project\" && exit 1",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf",
    "build:en": "ng build --prod --aot --locale en-US --i18n-format xlf --i18n-file src/locale/messages.en-US.xlf --output-path ../web/dist/en-US",
    "build:af": "ng build --prod --aot --locale af-ZA --i18n-format xlf --i18n-file src/locale/messages.af-ZA.xlf --output-path ../web/dist/af-ZA",
    "build-i18n": "npm run build:en && npm run build:af",
    "generate-vapid-keys": "web-push generate-vapid-keys --json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch --no-git-tag-version && git add package.json package-lock.json"
    }
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.9.0-beta.3",
    "@angular/animations": "^7.0.0-beta.5",
    "@angular/cdk": "^7.0.0-beta.0",
    "@angular/cli": "^7.0.0-beta.3",
    "@angular/common": "^7.0.0-beta.5",
    "@angular/compiler": "^7.0.0-beta.5",
    "@angular/compiler-cli": "^7.0.0-beta.5",
    "@angular/core": "^7.0.0-beta.5",
    "@angular/forms": "^7.0.0-beta.5",
    "@angular/http": "^7.0.0-beta.5",
    "@angular/material": "^7.0.0-beta.0",
    "@angular/platform-browser": "^7.0.0-beta.5",
    "@angular/platform-browser-dynamic": "^7.0.0-beta.5",
    "@angular/pwa": "^0.9.0-beta.3",
    "@angular/router": "^7.0.0-beta.5",
    "@angular/service-worker": "^7.0.0-beta.5",
    "@sendgrid/mail": "^6.3.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "concurrently": "^4.0.1",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "emojione": "^4.0.0",
    "emojione-assets": "^4.0.0",
    "express": "^4.16.3",
    "firebase": "^5.5.3",
    "hammerjs": "^2.0.8",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "marked": "^0.5.1",
    "material-design-icons": "^3.0.1",
    "neo4j-driver": "^1.6.3",
    "ngx-clipboard": "^11.1.7",
    "ngx-device-detector": "^1.3.3",
    "quill": "^1.3.6",
    "rxjs": "^6.3.3",
    "sanitize-html": "^1.19.1",
    "stripe": "^6.12.1",
    "ts-loader": "^5.2.1",
    "typescript": "~3.0.3",
    "uuid": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "ws": "^6.1.0",
    "zone.js": "^0.8.26",
    "winston": "^3.1.0",
    "web-push": "^3.3.3"
  },
  "devDependencies": {
    "@angular/language-service": "^7.0.0-beta.5",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/emojione": "^2.2.2",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.9",
    "@types/helmet": "^0.0.42",
    "@types/jasminewd2": "~2.0.5",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/marked": "0.4.2",
    "@types/node": "^10.11.4",
    "@types/quill": "^2.0.0",
    "@types/sanitize-html": "^1.18.2",
    "@types/stripe": "^6.0.1",
    "@types/stripe-v3": "^3.0.8",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.14",
    "@types/ws": "^6.0.1",
    "@types/winston": "^2.4.4",
    "@types/web-push": "^3.3.0",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "nodemon": "^1.18.4",
    "protractor": "~5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "husky": "^1.1.1"
  }
}
