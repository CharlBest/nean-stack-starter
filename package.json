{
  "name": "nean-stack-starter",
  "version": "0.0.492",
  "description": "A sample Angular, Node.js and Neo4j demo app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j",
    "heroku"
  ],
  "engines": {
    "node": "8.11.x"
  },
  "scripts": {
    "dev": "concurrently \"ng serve --aot\" \"npm run build:server:watch\" \"npm run start:server:watch\"",
    "build": "ng build --prod && webpack --mode=production",
    "build:server": "webpack --mode=development",
    "build:server:watch": "webpack --mode=development --watch",
    "start": "node dist/server/app.bundle.js",
    "start:server:watch": "nodemon dist/server/app.bundle.js",
    "heroku-postbuild": "npm run build",
    "ng": "ng",
    "test": "echo \"Error: no test... minimal project\" && exit 1",
    "lint": "echo \"Error: no lint... minimal project\" && exit 1",
    "e2e": "echo \"Error: no e2e... minimal project\" && exit 1",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf",
    "build:en": "ng build --prod --aot --locale en-US --i18n-format xlf --i18n-file src/locale/messages.en-US.xlf --output-path ../web/dist/en-US",
    "build:af": "ng build --prod --aot --locale af-ZA --i18n-format xlf --i18n-file src/locale/messages.af-ZA.xlf --output-path ../web/dist/af-ZA",
    "build-i18n": "npm run build:en && npm run build:af"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch --no-git-tag-version && git add package.json package-lock.json"
    }
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/animations": "^6.1.4",
    "@angular/cdk": "^6.4.6",
    "@angular/cli": "^6.1.5",
    "@angular/common": "^6.1.4",
    "@angular/compiler": "^6.1.1",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/core": "^6.1.4",
    "@angular/forms": "^6.1.4",
    "@angular/http": "^6.1.4",
    "@angular/material": "^6.4.6",
    "@angular/platform-browser": "^6.1.4",
    "@angular/platform-browser-dynamic": "^6.1.4",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.1.4",
    "@angular/service-worker": "^6.1.4",
    "@sendgrid/mail": "^6.3.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "concurrently": "^3.6.1",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "emojione": "^3.1.7",
    "emojione-assets": "^3.1.1",
    "express": "^4.16.3",
    "firebase": "^5.4.1",
    "hammerjs": "^2.0.8",
    "helmet": "^3.13.0",
    "husky": "^1.0.0-rc.13",
    "jsonwebtoken": "^8.3.0",
    "marked": "^0.4.0",
    "material-design-icons": "^3.0.1",
    "neo4j-driver": "^1.6.3",
    "ngx-clipboard": "^11.1.3",
    "ngx-device-detector": "^1.3.1",
    "quill": "^1.3.6",
    "rxjs": "^6.2.2",
    "stripe": "^6.8.0",
    "ts-loader": "^4.5.0",
    "typescript": "~2.7.0",
    "uuid": "^3.3.2",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2",
    "ws": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/language-service": "^6.1.4",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/emojione": "^2.2.2",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.116",
    "@types/marked": "0.4.0",
    "@types/node": "^10.9.3",
    "@types/quill": "^1.3.9",
    "@types/stripe": "^5.0.22",
    "@types/stripe-v3": "^3.0.8",
    "@types/uuid": "^3.4.3",
    "@types/webpack": "^4.4.11",
    "@types/ws": "^5.1.2",
    "codelyzer": "^4.4.4",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "nodemon": "^1.18.3",
    "protractor": "~5.3.2",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0"
  }
}
