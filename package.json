{
  "name": "nean-stack-starter",
  "version": "0.0.87",
  "description": "A sample Angular, Node.js and Neo4j demo app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j",
    "heroku"
  ],
  "engines": {
    "node": "8.11.x"
  },
  "scripts": {
    "dev": "concurrently \"ng serve --aot\" \"webpack --watch\" \"npm run start-watch\"",
    "build": "ng build --prod && webpack --mode=production",
    "build-dev-server": "webpack --mode=development",
    "start": "node dist/server/app.bundle.js",
    "start-watch": "nodemon dist/server/app.bundle.js",
    "heroku-postbuild": "npm run build",
    "ng": "ng",
    "test": "echo \"Error: no test... minimal project\" && exit 1",
    "lint": "echo \"Error: no lint... minimal project\" && exit 1",
    "e2e": "echo \"Error: no e2e... minimal project\" && exit 1",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf",
    "build:en": "ng build --prod --aot --locale en-US --i18n-format xlf --i18n-file src/locale/messages.en-US.xlf --output-path ../web/dist/en-US",
    "build:af": "ng build --prod --aot --locale af-ZA --i18n-format xlf --i18n-file src/locale/messages.af-ZA.xlf --output-path ../web/dist/af-ZA",
    "build-i18n": "npm run build:en && npm run build:af"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch --no-git-tag-version && git add package.json package-lock.json"
    }
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/animations": "^6.0.6",
    "@angular/cdk": "^6.3.0",
    "@angular/cli": "^6.0.8",
    "@angular/common": "^6.0.6",
    "@angular/compiler": "^6.0.6",
    "@angular/compiler-cli": "^6.0.6",
    "@angular/core": "^6.0.6",
    "@angular/forms": "^6.0.6",
    "@angular/http": "^6.0.6",
    "@angular/material": "^6.3.0",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.6",
    "@sendgrid/mail": "^6.3.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "concurrently": "^3.6.0",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "emojione": "^3.1.6",
    "express": "^4.16.3",
    "firebase": "^5.1.0",
    "hammerjs": "^2.0.8",
    "husky": "^1.0.0-rc.8",
    "jsonwebtoken": "^8.3.0",
    "marked": "^0.4.0",
    "material-design-icons": "^3.0.1",
    "neo4j-driver": "^1.6.1",
    "ngx-clipboard": "^11.1.1",
    "quill": "^1.3.6",
    "rxjs": "^6.2.1",
    "stripe": "^6.1.1",
    "ts-loader": "^4.4.1",
    "typescript": "~2.7.0",
    "uuid": "^3.2.1",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2",
    "ws": "^5.2.0",
    "zone.js": "^0.8.26",
    "@angular/service-worker": "^6.0.6"
  },
  "devDependencies": {
    "@angular/language-service": "^6.0.6",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/emojione": "^2.2.2",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/lodash": "^4.14.110",
    "@types/marked": "0.4.0",
    "@types/node": "~10.3.4",
    "@types/uuid": "^3.4.3",
    "@types/quill": "^1.3.6",
    "@types/stripe": "^5.0.15",
    "@types/webpack": "^4.4.2",
    "@types/ws": "^5.1.2",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "nodemon": "^1.17.5",
    "protractor": "~5.3.2",
    "ts-node": "~6.1.2",
    "tslint": "~5.10.0"
  }
}
