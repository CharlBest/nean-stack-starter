{
  "name": "nean",
  "version": "0.0.4",
  "description": "A sample Angular, Node.js and Neo4j app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j",
    "heroku"
  ],
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"ng build --watch\" \"webpack --watch\" \"nodemon dist/server/app.bundle.js\"",
    "start": "node dist/server/app.bundle.js",
    "start-watch": "nodemon dist/server/app.bundle.js",
    "start-prod": "set NODE_ENV=production&&node dist/server/app.bundle.js",
    "build": "ng build --prod && webpack",
    "postinstall": "ng build --prod && webpack",
    "test": "echo \"Error: no test... minimal project\" && exit 1",
    "lint": "echo \"Error: no lint... minimal project\" && exit 1",
    "e2e": "echo \"Error: no e2e... minimal project\" && exit 1",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf",
    "build:en": "ng build --prod --aot --locale en-US --i18n-format xlf --i18n-file src/locale/messages.en-US.xlf --output-path ../web/dist/en-US",
    "build:af": "ng build --prod --aot --locale af-ZA --i18n-format xlf --i18n-file src/locale/messages.af-ZA.xlf --output-path ../web/dist/af-ZA",
    "build-i18n": "npm run build:en && npm run build:af"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.1",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/cli": "^1.5.0",
    "@angular/common": "^5.0.1",
    "@angular/compiler": "^5.0.1",
    "@angular/compiler-cli": "^5.0.1",
    "@angular/core": "^5.0.1",
    "@angular/forms": "^5.0.1",
    "@angular/http": "^5.0.1",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.1",
    "@angular/platform-browser-dynamic": "^5.0.1",
    "@angular/router": "^5.0.1",
    "@sendgrid/mail": "^6.1.4",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "emojione": "^3.1.2",
    "express": "^4.16.2",
    "firebase": "^4.6.2",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.1.0",
    "marked": "^0.3.6",
    "neo4j-driver": "^1.5.0",
    "ngx-clipboard": "^8.1.1",
    "node-uuid": "^1.4.7",
    "quill": "^1.3.3",
    "rxjs": "^5.5.2",
    "stripe": "^5.3.0",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "ws": "^3.3.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/language-service": "^5.0.1",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/cors": "^2.8.2",
    "@types/emojione": "^2.2.1",
    "@types/express": "^4.0.39",
    "@types/firebase": "^2.4.32",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/lodash": "^4.14.84",
    "@types/marked": "0.3.0",
    "@types/node": "^8.0.50",
    "@types/node-uuid": "^0.0.28",
    "@types/quill": "^1.3.3",
    "@types/stripe": "^4.7.2",
    "@types/webpack": "^3.8.1",
    "@types/ws": "^3.2.0",
    "codelyzer": "^4.0.1",
    "nodemon": "^1.12.1",
    "tslint": "^5.8.0",
    "webpack-node-externals": "^1.6.0"
  }
}
