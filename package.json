{
  "name": "nean-stack-starter",
  "version": "0.0.1115",
  "description": "A sample Angular, Node.js and Neo4j demo app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j"
  ],
  "scripts": {
    "dev": "concurrently \"ng serve --open\" \"webpack --mode=development --watch\" \"npm run start:server:watch\"",
    "build": "ng build --configuration=production && webpack --mode=production && npm run minify-index-html && npm run minify-email-template-html",
    "buildv2": "bash build.sh",
    "start": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/web/web.bundle.js",
    "start:worker": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/worker/worker.bundle.js",
    "start:worker:watch": "nodemon --max_semi_space_size=2 --max_old_space_size=256 dist/server/worker/worker.bundle.js",
    "start:server:watch": "nodemon --inspect --max_semi_space_size=2 --max_old_space_size=256 dist/server/web/web.bundle.js",
    "test": "ts-node --project=src/client/tsconfig.spec.json node_modules/jasmine/bin/jasmine --config=src/client/test/jasmine.json",
    "lint": "ng lint",
    "generate-vapid-keys": "web-push generate-vapid-keys --json",
    "minify-index-html": "html-minifier dist/client/index.html --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --use-short-doctype --minify-css true --minify-js true -o dist/client/index.html",
    "minify-email-template-html": "html-minifier --input-dir src/server/worker/email-templates --collapse-whitespace --minify-css true --output-dir dist/server/worker/email-templates"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch --no-git-tag-version && git add package.json package-lock.json"
    }
  },
  "dependencies": {
    "@angular/animations": "^8.2.7",
    "@angular/cdk": "^8.2.0",
    "@angular/cdk-experimental": "^8.2.0",
    "@angular/common": "^8.2.7",
    "@angular/compiler": "^8.2.7",
    "@angular/core": "^8.2.7",
    "@angular/forms": "^8.2.7",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "^8.2.7",
    "@angular/platform-browser-dynamic": "^8.2.7",
    "@angular/pwa": "^0.803.5",
    "@angular/router": "^8.2.7",
    "@angular/service-worker": "^8.2.7",
    "@github/webauthn-json": "0.3.1",
    "amqplib": "^0.5.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "browser-image-compression": "^1.0.6",
    "compression": "^1.7.4",
    "countly-sdk-web": "^19.8.0",
    "emojione": "^4.5.0",
    "emojione-assets": "^4.5.0",
    "express": "^4.17.1",
    "firebase": "^6.6.2",
    "hammerjs": "^2.0.8",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "marked": "^0.7.0",
    "material-design-icons": "^3.0.1",
    "neo4j-driver": "^1.7.6",
    "otplib": "^11.0.1",
    "qrcode": "^1.4.2",
    "quill": "^1.3.7",
    "rxjs": "^6.5.3",
    "sanitize-html": "^1.20.1",
    "stripe": "^7.9.1",
    "ts-loader": "^6.1.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "web-push": "^3.3.5",
    "webpack-cli": "^3.3.9",
    "winston": "^3.2.1",
    "ws": "^7.1.2",
    "zone.js": "^0.10.2",
    "nodemailer": "^6.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.5",
    "@angular/cli": "^8.3.5",
    "@angular/compiler-cli": "^8.2.7",
    "@angular/language-service": "^8.2.7",
    "@types/amqplib": "^0.5.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/emojione": "^2.2.5",
    "@types/express": "^4.17.1",
    "@types/hammerjs": "~2.0.36",
    "@types/helmet": "^0.0.44",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/marked": "0.6.5",
    "@types/node": "^12.7.5",
    "@types/qrcode": "^1.3.4",
    "@types/quill": "^2.0.2",
    "@types/sanitize-html": "^1.20.2",
    "@types/stripe": "^6.32.2",
    "@types/stripe-v3": "^3.1.7",
    "@types/uuid": "^3.4.5",
    "@types/web-push": "^3.3.0",
    "@types/webpack": "^4.39.1",
    "@types/winston": "^2.4.4",
    "@types/ws": "^6.0.3",
    "@types/puppeteer": "^1.19.1",
    "@types/jasmine": "^3.4.0",
    "@types/nodemailer": "^6.2.1",
    "codelyzer": "^5.1.1",
    "html-minifier": "^4.0.0",
    "husky": "^3.0.5",
    "nodemon": "^1.19.2",
    "ts-node": "^8.4.1",
    "tslint": "~5.20.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.5.3",
    "puppeteer": "^1.20.0",
    "jasmine": "^3.5.0",
    "concurrently": "^4.1.2"
  }
}
