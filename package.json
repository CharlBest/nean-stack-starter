{
  "name": "nean-stack-starter",
  "version": "0.0.1138",
  "description": "A sample Angular, Node.js and Neo4j demo app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j"
  ],
  "scripts": {
    "dev": "concurrently \"ng serve --open\" \"webpack --mode=development --watch\" \"npm run start:server:watch\" \"npm run start:services\"",
    "build": "ng build --configuration=production && webpack --mode=production && npm run minify-index-html",
    "buildv2": "bash build.sh",
    "build:server": "webpack --mode=development",
    "start": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/web/web.bundle.js",
    "start:worker": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/worker/worker.bundle.js",
    "start:worker:watch": "nodemon --max_semi_space_size=2 --max_old_space_size=256 dist/server/worker/worker.bundle.js",
    "start:server:watch": "nodemon --inspect --max_semi_space_size=2 --max_old_space_size=256 dist/server/web/web.bundle.js",
    "start:services": "docker-compose --file ./services/docker-compose.yml --env-file=./services/.env up --detach",
    "test": "ts-node --project=src/client/tsconfig.spec.json node_modules/jasmine/bin/jasmine --config=src/client/test/jasmine.json",
    "lint": "ng lint",
    "generate-vapid-keys": "web-push generate-vapid-keys --json",
    "minify-index-html": "html-minifier dist/client/index.html --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --use-short-doctype --minify-css true --minify-js true -o dist/client/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch --no-git-tag-version && git add package.json package-lock.json"
    }
  },
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/forms": "^9.0.7",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/pwa": "^0.900.7",
    "@angular/router": "^9.0.7",
    "@angular/service-worker": "^9.0.7",
    "@github/webauthn-json": "0.4.1",
    "amqplib": "^0.5.5",
    "bcryptjs": "^2.4.3",
    "browser-image-compression": "^1.0.8",
    "compression": "^1.7.4",
    "countly-sdk-web": "^19.8.0",
    "emojione": "^4.5.0",
    "emojione-assets": "^4.5.0",
    "express": "^4.17.1",
    "firebase": "^7.12.0",
    "hammerjs": "^2.0.8",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "marked": "^0.8.2",
    "material-design-icons": "^3.0.1",
    "neo4j-driver": "^1.7.6",
    "otplib": "^12.0.1",
    "qrcode": "^1.4.4",
    "quill": "^1.3.7",
    "rxjs": "^6.5.4",
    "sanitize-html": "^1.22.1",
    "stripe": "^7.9.1",
    "ts-loader": "^6.2.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.11.1",
    "uuid": "^7.0.2",
    "web-push": "^3.4.3",
    "webpack-cli": "^3.3.11",
    "winston": "^3.2.1",
    "ws": "^7.2.3",
    "zone.js": "^0.10.3",
    "nodemailer": "^6.4.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.7",
    "@types/amqplib": "^0.5.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/emojione": "^2.2.6",
    "@types/express": "^4.17.3",
    "@types/hammerjs": "~2.0.36",
    "@types/helmet": "^0.0.45",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/marked": "0.7.3",
    "@types/node": "^12.12.31",
    "@types/nodemailer": "^6.4.0",
    "@types/puppeteer": "^2.0.1",
    "@types/qrcode": "^1.3.4",
    "@types/quill": "^2.0.3",
    "@types/sanitize-html": "^1.22.0",
    "@types/stripe": "^6.32.2",
    "@types/stripe-v3": "^3.1.7",
    "@types/uuid": "^7.0.2",
    "@types/web-push": "^3.3.0",
    "@types/webpack": "^4.41.8",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.2.3",
    "codelyzer": "^5.2.1",
    "concurrently": "^5.1.0",
    "html-loader": "^1.0.0",
    "html-minifier": "^4.0.0",
    "husky": "^4.2.3",
    "nodemon": "^2.0.2",
    "puppeteer": "^2.1.1",
    "ts-node": "^8.8.1",
    "tslint": "~6.1.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.7.0"
  }
}
