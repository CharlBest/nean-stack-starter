{
  "name": "nean-stack-starter",
  "version": "0.0.5",
  "description": "A sample Angular, Node.js and Neo4j app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j",
    "heroku"
  ],
  "engines": {
    "node": "8.11.x"
  },
  "scripts": {
    "dev": "concurrently \"ng serve --aot\" \"webpack --watch\" \"npm run start-watch\"",
    "build": "ng build --prod && webpack",
    "start": "node dist/server/app.bundle.js",
    "start-watch": "nodemon dist/server/app.bundle.js",
    "heroku-postbuild": "npm run build",
    "ng": "ng",
    "test": "echo \"Error: no test... minimal project\" && exit 1",
    "lint": "echo \"Error: no lint... minimal project\" && exit 1",
    "e2e": "echo \"Error: no e2e... minimal project\" && exit 1",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf",
    "build:en": "ng build --prod --aot --locale en-US --i18n-format xlf --i18n-file src/locale/messages.en-US.xlf --output-path ../web/dist/en-US",
    "build:af": "ng build --prod --aot --locale af-ZA --i18n-format xlf --i18n-file src/locale/messages.af-ZA.xlf --output-path ../web/dist/af-ZA",
    "build-i18n": "npm run build:en && npm run build:af"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.6.7",
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.1.0",
    "@angular/cli": "^6.0.7",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@sendgrid/mail": "^6.1.4",
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "concurrently": "^3.5.0",
    "core-js": "^2.5.4",
    "cors": "^2.8.1",
    "emojione": "^3.1.2",
    "express": "^4.16.2",
    "firebase": "^4.6.2",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.1.0",
    "marked": "^0.3.7",
    "neo4j-driver": "^1.5.1",
    "ngx-clipboard": "^11.1.0",
    "node-uuid": "^1.4.7",
    "quill": "^1.3.3",
    "rxjs": "^6.0.0",
    "stripe": "^5.4.0",
    "ts-loader": "^3.2.0",
    "typescript": "~2.7.0",
    "webpack-node-externals": "^1.6.0",
    "ws": "^3.3.2",
    "zone.js": "^0.8.26",
    "webpack-cli": "^3.0.1"
  },
  "devDependencies": {
    "@angular/language-service": "^6.0.3",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/cors": "^2.8.3",
    "@types/emojione": "^2.2.1",
    "@types/express": "^4.0.39",
    "@types/firebase": "^2.4.32",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jsonwebtoken": "^7.2.4",
    "@types/lodash": "^4.14.88",
    "@types/marked": "0.3.0",
    "@types/node": "~8.9.4",
    "@types/node-uuid": "^0.0.28",
    "@types/quill": "^1.3.5",
    "@types/stripe": "^4.9.0",
    "@types/webpack": "^3.8.1",
    "@types/ws": "^3.2.1",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.12.5",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}
