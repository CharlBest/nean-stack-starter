{
  "name": "nean-stack-starter",
  "version": "0.0.913",
  "description": "A sample Angular, Node.js and Neo4j demo app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j"
  ],
  "scripts": {
    "dev": "concurrently \"npm run start:client:watch\" \"npm run build:server:watch\" \"npm run start:server:watch\"",
    "build": "ng build --configuration=production && webpack --mode=production",
    "build:dev": "ng build --configuration=development && webpack --mode=production",
    "build:server:watch": "webpack --mode=development --watch",
    "start": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/web/web.bundle.js",
    "start:worker": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/worker/worker.bundle.js",
    "start:worker:watch": "nodemon --max_semi_space_size=2 --max_old_space_size=256 dist/server/worker/worker.bundle.js",
    "start:client:watch": "ng serve",
    "start:server:watch": "nodemon --inspect --max_semi_space_size=2 --max_old_space_size=256 dist/server/web/web.bundle.js",
    "ng": "ng",
    "test": "echo \"Error: no test... minimal project\" && exit 1",
    "lint": "ng lint",
    "e2e": "echo \"Error: no e2e... minimal project\" && exit 1",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf",
    "build:en": "ng build --prod --locale en-US --i18n-format xlf --i18n-file src/locale/messages.en-US.xlf --output-path ../web/dist/en-US",
    "build:af": "ng build --prod --locale af-ZA --i18n-format xlf --i18n-file src/locale/messages.af-ZA.xlf --output-path ../web/dist/af-ZA",
    "build-i18n": "npm run build:en && npm run build:af",
    "generate-vapid-keys": "web-push generate-vapid-keys --json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch --no-git-tag-version && git add package.json package-lock.json"
    }
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.800.6",
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/cli": "^8.0.6",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/pwa": "^0.800.6",
    "@angular/router": "^8.0.3",
    "@angular/service-worker": "^8.0.3",
    "@sendgrid/mail": "^6.4.0",
    "amqplib": "^0.5.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "emojione": "^4.5.0",
    "emojione-assets": "^4.5.0",
    "express": "^4.17.1",
    "firebase": "^6.2.4",
    "hammerjs": "^2.0.8",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "marked": "^0.6.3",
    "material-design-icons": "^3.0.1",
    "neo4j-driver": "^1.7.5",
    "ngx-clipboard": "^12.1.2",
    "quill": "^1.3.6",
    "rxjs": "^6.5.2",
    "sanitize-html": "^1.20.1",
    "stripe": "^7.4.0",
    "ts-loader": "^6.0.4",
    "tslib": "^1.10.0",
    "typescript": "~3.4.3",
    "uuid": "^3.3.2",
    "web-push": "^3.3.5",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.2.1",
    "ws": "^7.0.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/language-service": "^8.0.3",
    "@types/amqplib": "^0.5.13",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/emojione": "^2.2.5",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/marked": "0.6.5",
    "@types/node": "^12.0.10",
    "@types/quill": "^2.0.2",
    "@types/sanitize-html": "^1.20.0",
    "@types/stripe": "^6.30.5",
    "@types/stripe-v3": "^3.1.3",
    "@types/uuid": "^3.4.4",
    "@types/web-push": "^3.3.0",
    "@types/webpack": "^4.4.34",
    "@types/winston": "^2.4.4",
    "@types/ws": "^6.0.1",
    "codelyzer": "^5.1.0",
    "husky": "^2.7.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint": "~5.18.0"
  }
}
