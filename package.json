{
  "name": "nean-stack-starter",
  "version": "0.0.1080",
  "description": "A sample Angular, Node.js and Neo4j demo app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlBest/nean-stack-starter"
  },
  "keywords": [
    "angular",
    "node",
    "neo4j"
  ],
  "scripts": {
    "dev": "concurrently \"npm run start:client:watch\" \"npm run build:server:watch\" \"npm run start:server:watch\"",
    "build": "ng build --configuration=production && webpack --mode=production && npm run minify-index-html",
    "buildv2": "bash build.sh",
    "build:dev": "ng build --configuration=development && webpack --mode=production",
    "build:server:watch": "webpack --mode=development --watch",
    "start": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/web/web.bundle.js",
    "start:worker": "node --max_semi_space_size=2 --max_old_space_size=256 dist/server/worker/worker.bundle.js",
    "start:worker:watch": "nodemon --max_semi_space_size=2 --max_old_space_size=256 dist/server/worker/worker.bundle.js",
    "start:client:watch": "ng serve --open",
    "start:server:watch": "nodemon --inspect --max_semi_space_size=2 --max_old_space_size=256 dist/server/web/web.bundle.js",
    "ng": "ng",
    "test": "echo \"Error: no test... minimal project\" && exit 1",
    "lint": "ng lint",
    "e2e": "echo \"Error: no e2e... minimal project\" && exit 1",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf",
    "build:en": "ng build --prod --locale en-US --i18n-format xlf --i18n-file src/locale/messages.en-US.xlf --output-path ../web/dist/en-US",
    "build:af": "ng build --prod --locale af-ZA --i18n-format xlf --i18n-file src/locale/messages.af-ZA.xlf --output-path ../web/dist/af-ZA",
    "build-i18n": "npm run build:en && npm run build:af",
    "generate-vapid-keys": "web-push generate-vapid-keys --json",
    "minify-index-html": "html-minifier dist/client/index.html --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --use-short-doctype --minify-css true --minify-js true -o dist/client/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch --no-git-tag-version && git add package.json package-lock.json"
    }
  },
  "dependencies": {
    "@angular/animations": "^8.2.5",
    "@angular/cdk": "^8.2.0",
    "@angular/cdk-experimental": "^8.2.0",
    "@angular/common": "^8.2.5",
    "@angular/compiler": "^8.2.5",
    "@angular/core": "^8.2.5",
    "@angular/forms": "^8.2.5",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "^8.2.5",
    "@angular/platform-browser-dynamic": "^8.2.5",
    "@angular/pwa": "^0.803.3",
    "@angular/router": "^8.2.5",
    "@angular/service-worker": "^8.2.5",
    "@github/webauthn-json": "^0.3.1",
    "@sendgrid/mail": "^6.4.0",
    "amqplib": "^0.5.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "browser-image-compression": "^1.0.6",
    "compression": "^1.7.4",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "countly-sdk-web": "^19.8.0",
    "emojione": "^4.5.0",
    "emojione-assets": "^4.5.0",
    "express": "^4.17.1",
    "firebase": "^6.6.0",
    "hammerjs": "^2.0.8",
    "helmet": "^3.21.0",
    "jsonwebtoken": "^8.5.1",
    "marked": "^0.7.0",
    "material-design-icons": "^3.0.1",
    "neo4j-driver": "^1.7.6",
    "otplib": "^11.0.1",
    "qrcode": "^1.4.1",
    "quill": "^1.3.7",
    "rxjs": "^6.5.2",
    "sanitize-html": "^1.20.1",
    "stripe": "^7.9.0",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "web-push": "^3.3.5",
    "webpack-cli": "^3.3.8",
    "winston": "^3.2.1",
    "ws": "^7.1.2",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.3",
    "@angular/cli": "^8.3.3",
    "@angular/compiler-cli": "^8.2.5",
    "@angular/language-service": "^8.2.5",
    "@types/amqplib": "^0.5.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/emojione": "^2.2.5",
    "@types/express": "^4.17.1",
    "@types/hammerjs": "~2.0.36",
    "@types/helmet": "^0.0.44",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/marked": "0.6.5",
    "@types/node": "^12.7.4",
    "@types/qrcode": "^1.3.4",
    "@types/quill": "^2.0.2",
    "@types/sanitize-html": "^1.20.1",
    "@types/stripe": "^6.32.0",
    "@types/stripe-v3": "^3.1.7",
    "@types/uuid": "^3.4.5",
    "@types/web-push": "^3.3.0",
    "@types/webpack": "^4.39.1",
    "@types/winston": "^2.4.4",
    "@types/ws": "^6.0.3",
    "codelyzer": "^5.1.0",
    "html-minifier": "^4.0.0",
    "husky": "^3.0.5",
    "nodemon": "^1.19.2",
    "ts-node": "^8.3.0",
    "tslint": "~5.20.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.5.3"
  }
}
