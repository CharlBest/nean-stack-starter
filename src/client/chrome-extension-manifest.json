{
    "manifest_version": 2,
    "name": "NEAN",
    "short_name": "NEAN",
    "description": "App extension for chrome",
    "version": "0.0.5",
    "browser_action": {
        "default_icon": "assets/logo.png",
        "default_popup": "index.extension.html"
    },
    "icons": {
        "16": "icons/favicon-16x16.png",
        "48": "icons/android-icon-48x48.png",
        "128": "icons/android-icon-128x128.png"
    },
    "permissions": [
        "https://nean.io/"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://checkout.stripe.com; object-src 'self'",
    "web_accessible_resources": [
        "assets/*"
    ]
}